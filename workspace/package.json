{
  "private": true,
  "scripts": {
    "clean": "rimraf _site/* _transpiled/*",
    "dev:vite": "vite",
    "dev:jsx": "babel ./src/views --watch --extensions '.js,.ts,.tsx' --out-dir _transpiled --copy-files",
    "dev:11ty": "node --require esbuild-register node_modules/.bin/eleventy --serve",
    "start": "NODE_ENV=development run-p dev:*",
    "build:jsx": "babel ./src/views --extensions '.js,.ts,.tsx' --out-dir _transpiled --copy-files",
    "build:vite": "vite build",
    "build:11ty": "node --require esbuild-register node_modules/.bin/eleventy",
    "build": "NODE_ENV=production run-s clean build:*",
    "type-check": "tsc",
    "fix:prettier": "prettier --write ./src/**/*.{ts,tsx,svelte}",
    "fix:eslint": "eslint './src/**/*.{ts,tsx,svelte}' -c .eslintrc.json --fix",
    "fix": "run-s fix:*"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^3.1.0",
    "@babel/cli": "^7.20.7",
