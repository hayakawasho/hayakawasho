---
import Layout from '~/layout.astro';
import WorksIndex from '~/_features/page.works/view';
import dotenv from 'dotenv';
import { WorksRepository } from '~/_repositories/works';

dotenv.config();

const repository = WorksRepository.create(process.env.API_KEY!);

const result = await repository.findList({
  limit: 99,
  orders: '-launch',
});
---

<Layout title="Works" permalink=''>
  <link
    slot="prependHead"
    as="image"
    crossorigin="anonymous"
    href=""
    media=""
    rel="preload"
  />
  <WorksIndex />
</Layout>
